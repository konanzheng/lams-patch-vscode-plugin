{
	"name": "lams-patch",
	"displayName": "lams-patch",
	"description": "make patch from git history for maven web project",
	"version": "0.0.4",
	"publisher": "ruizhaozheng",
	"icon": "logo.png",
	"author": {
		"name": "郑瑞召",
		"email": "ruizhaozheng@aliyun.com",
		"url": "http://konanzheng.gitee.io/",
		"organization": "konanzheng"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/konanzheng/lams-patch-vscode-plugin"
	},
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:lams-patch.makePatch"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "lams-patch.makePatch",
				"title": "make patch(生成补丁包)",
				"category": "lams-patch",
				"icon":"$(source-control-view-icon)"

			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
